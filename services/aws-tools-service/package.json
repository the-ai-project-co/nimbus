{
  "name": "@nimbus/aws-tools-service",
  "version": "0.1.0",
  "type": "module",
  "description": "AWS Tools Service for Nimbus",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "echo 'Lint not configured yet'",
    "type-check": "tsc --noEmit",
    "build": "bun build src/index.ts --outdir=dist --target=bun"
  },
  "dependencies": {
    "@aws-sdk/client-account": "^3.980.0",
    "@aws-sdk/client-cloudfront": "^3.980.0",
    "@aws-sdk/client-dynamodb": "^3.980.0",
    "@aws-sdk/client-ec2": "^3.980.0",
    "@aws-sdk/client-ecs": "^3.980.0",
    "@aws-sdk/client-eks": "^3.980.0",
    "@aws-sdk/client-iam": "^3.980.0",
    "@aws-sdk/client-lambda": "^3.980.0",
    "@aws-sdk/client-rds": "^3.980.0",
    "@aws-sdk/client-resource-groups-tagging-api": "^3.980.0",
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/client-sts": "^3.980.0",
    "@aws-sdk/credential-providers": "^3.980.0",
    "@nimbus/shared-clients": "workspace:*",
    "@nimbus/shared-types": "workspace:*",
    "@nimbus/shared-utils": "workspace:*",
    "ini": "^4.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.3"
  }
}