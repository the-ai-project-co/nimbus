FALLBACK_PROVIDERS=anthropic,openai,google

# Service Discovery
STATE_SERVICE_URL=http://localhost:3011
LLM_SERVICE_URL=http://localhost:3002
CORE_ENGINE_SERVICE_URL=http://localhost:3001